CREATE TABLE IF NOT EXISTS users (
                user_id TEXT PRIMARY KEY,
                msg_count INTEGER DEFAULT 0,
                xp INTEGER 0,
                call_sec INTEGER DEFAULT 0,
                cookies INTEGER DEFAULT 0)

